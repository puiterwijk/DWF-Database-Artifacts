  <script>
function toggle_wrap_comments (unwrap) {
    var elements = YAHOO.util.Dom.getElementsByClassName('bz_comment_text');
    for (var i = 0; i < elements.length; i++) {
        if (unwrap) {
            YAHOO.util.Dom.removeClass(elements[i], 'bz_wrap_comment_text');
        }
        else {
            YAHOO.util.Dom.addClass(elements[i], 'bz_wrap_comment_text');
        }
    }

    var thelink = document.getElementById('wrap_comment_link');
    if (unwrap) {
        thelink.onclick = function() { toggle_wrap_comments(0); return false; }
        thelink.innerHTML = 'Wrap comment';
    }
    else {
        thelink.onclick = function() { toggle_wrap_comments(1); return false; }
        thelink.innerHTML = 'Unwrap comment';
    }
}
</script>


<script>
var mysel = document.getElementsByClassName('flag_type-415')[0];
if ( document.getElementById('cf_release_notes').value != '' &&
     document.getElementById('cf_release_notes').value != cf_doc_type_text[document.getElementById('cf_doc_type').value] &&
     mysel.options[mysel.selectedIndex].value != '+'
   )
    document.getElementById('cf_doc_warn').innerHTML = '<div class="warning "><b>Warning: Doc Text is not yet verified as correct</b></div>';
</script>

</td>
<td>
</td>
</tr></table>
  </div>

